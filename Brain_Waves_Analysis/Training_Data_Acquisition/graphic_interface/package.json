{
  "name": "training_data_acquisition_gui",
  "author": "Vinicius de A. L. e Souza",
  "description": "This is a graphic interface desktop app to help on the acquisition of training data from the OpenBCI's Cython board for the Cerebro project.",
  "version": "0.1.0",
  "private": true,
  "main": "./electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "electron-is-dev": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "npm run build && electron-builder -w"
  },
  "build": {
    "extends": null,
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "build/favicon.ico"
    },
    "target": "zip"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.2.2",
    "electron-builder": "^22.9.1",
    "wait-on": "^5.2.1"
  }
}